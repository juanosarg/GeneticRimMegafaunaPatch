<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Megafauna Incubators -->



	<RecipeDef>
		<defName>GR_CreateMegafaunaBearIncubator</defName>
		<label>megafauna bear incubator</label>
		<description>Fill an incubator with genes from a bear and a megafauna animal</description>
		<jobString>Filling Incubator</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>500</workAmount>
		<recipeUsers>

			<li>GR_MegafaunaGenePod</li>				
		</recipeUsers>	
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>GR_EmptyIncubator</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>GR_BearGenetic</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>GR_MegafaunaGenetic</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>

			<thingDefs>
				<li>GR_EmptyIncubator</li>
				<li>GR_BearGenetic</li>
				<li>GR_MegafaunaGenetic</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<GR_MegafaunaBearIncubator>1</GR_MegafaunaBearIncubator>
		</products>
		<researchPrerequisite>GR_MegafaunaGeneticEngineering</researchPrerequisite>
		<skillRequirements>
			<Intellectual>8</Intellectual>
		</skillRequirements>
	</RecipeDef>

